{
    "version": 2,
    "name": "Interview Project Jenk",
    "builds": [
      {
        "src": "interview/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "interview/build"
        }
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/interview/$1"
      }
    ]
  }
  